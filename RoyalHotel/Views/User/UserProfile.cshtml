@model RoyalHotel.Models.ViewModels.EditProfileViewModel
@{
    ViewData["Title"] = "UserProfile";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}

<h1>UserProfile</h1>




<div class="container">
    <h2>Current Profile Information</h2>

    <div class="form-group">
        <label>First Name:</label>
        <p>@Model.FirstName</p>
    </div>

    <div class="form-group">
        <label>Last Name:</label>
        <p>@Model.LastName</p>
    </div>

    <div class="form-group">
        <label>Email:</label>
        <p>@Model.Email</p>
    </div>

    <div class="form-group">
        <label>Phone:</label>
        <p>@Model.Phone</p>
    </div>
</div>
